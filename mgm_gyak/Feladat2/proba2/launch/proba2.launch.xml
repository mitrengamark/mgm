<launch>
  <!-- Node indítása -->
  <node pkg="proba2" exec="proba2_node" name="proba2_node" output="screen">
    <param name="use_sim_time" value="true"/>
  </node>

  <!-- RViz indítása a konfigurációval -->
  <node pkg="rviz2" exec="rviz2" name="rviz2" args="-d $(find-pkg-share proba2)/rviz/proba2.rviz">
    <param name="use_sim_time" value="true"/>
  </node>
</launch>
